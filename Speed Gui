local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
getgenv().Theme = "Synapse"
local Window = Library.CreateLib("Jorden Hub", getgenv().Theme)
local Tab = Window:NewTab("Local Player")
local Section = Tab:NewSection("Speed Script")
Section:NewTextBox("Character Speed", "Type A Number In And Make The Speed You Want", function(txt)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = txt
end)
Section:NewSlider("Speed Sllider", "Bring Up Speed", 250, 0, function(speed)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
end)

Section:NewKeybind("Toggle Speed Gui", "Make The Gui Minimised, Press Again To Make It Vissible Again", Enum.KeyCode.RightAlt, function()
	Library:ToggleUI()
end)
